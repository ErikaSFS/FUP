void selection_sort(int v[], int n){
int min_index=0;
for(int i=0; i<n-1;i++){

//achar o menor
   for(int j=i;j<n;j++){
     if(v[j] < v [min_index){
     min_index=j
     }
     }
 
 aux=v[min_index];
 v[min_index]=v[i];
 v[i]=aux;
 }
 
 Void bubble_sort(intv[],int n)[
 int aux;
 for (int i=0;i<n-1;i++){
 for (int j=0; j=n,j++){
     if(v[j]>v[j+1]){
     aux=v[j];
     v[j]=v[j+1];
     v[j+1]=aux;
     
     }
     }
